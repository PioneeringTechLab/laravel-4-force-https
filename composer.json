{
    "name": "csun-metalab/laravel-4-force-https",
    "description": "A small Composer package for Laravel 4.2 to force HTTPS in the URL via a route filter",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Fritz",
            "email": "mattf@burbankparanormal.com"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "minimum-stability": "stable",
    "autoload": {
        "classmap": [
            "src/ForceHttps4/"
        ],
        "psr-0": {
            "CSUNMetaLab\\ForceHttps4\\": "src/ForceHttps4/"
        }
    }
}
